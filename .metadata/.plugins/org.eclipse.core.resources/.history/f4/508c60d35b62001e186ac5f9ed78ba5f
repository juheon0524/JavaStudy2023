package ex05;

public class CarExample {

	public static void main(String[] args) {
		Car car = new Car();
		
		car.run1();
		System.out.println("==============");
		car.run2();
		
		car.run3(new Tire() {
			@Override
			public void roll() {
				System.out.println("익명 객체 3이 굴러감...");
			}
		});
	}

}
